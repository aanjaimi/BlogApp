// prisma/schema.prisma
datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Blog {
  id                 String   @id @default(cuid())
  title              String   @unique
  content            String
  createdAt          DateTime @default(now())
}
